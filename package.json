  "main": "src/server.js",
  "scripts": {
    "1": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/criar-compra.ts",
    "2": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/buscar.ts",
